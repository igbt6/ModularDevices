            icon="m_ic_mod_env_hum">
            <VarRead
                equation="d[4]+d[5]/100">
            </VarRead>          
        </Variable>
		<Variable 
            name="Temperature"     
             
            unit="°C"            
            icon="m_ic_mod_env_temp">
            <VarRead
                equation="d[6]+d[7]/100" >
            </VarRead>  
        </Variable>
		<Variable name="Ambient light"             
            unit="lux"               
            icon="m_ic_mod_env_light">
