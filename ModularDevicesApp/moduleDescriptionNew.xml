            </VarRead>  
        </Variable>
		<Variable name="Gyro X"                     
            unit=""         
            icon="m_ic_mod_env_gyro_x">
            <VarRead
                equation="d[29]*256+d[30]" >
            </VarRead>  
        </Variable>
		<Variable name="Gyro Y"          
            
            unit=""         
            icon="m_ic_mod_env_gyro_y">
            <VarRead
                equation="d[31]*256+d[32]" >
            </VarRead>  
